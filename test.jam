fn factorial(number) {
    if number < 0 {
        return "Factorial is not defined for negative numbers"
    }
    if number == 0 or number == 1 {
        return 1
    }

    let result = 1
    while number > 1 {
        result = result * number
        --number
    }

    return result
}

let minNumber = 1
--minNumber
--minNumber

let maxNumber = 10
let result;

while maxNumber > minNumber {
    result = factorial(maxNumber)
    println("Factorial of", maxNumber, "is", result)
    --maxNumber
}
