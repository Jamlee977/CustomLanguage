class Person {
    fn constructor(name, age) {
        this.name = name
        this.age = age
    }

    fn getName(self) {
        return self.name
    }

    fn getAge(self) {
        return self.age
    }

    fn setName(self, name) {
        self.name = name
    }

    fn setAge(self, age) {
        self.age = age
    }
}

const person = Person("Jam", 19)
person.setName(person, "Omar")
println(person.name)
println(person.getName(person))
